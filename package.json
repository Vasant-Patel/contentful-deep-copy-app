{
  "name": "contentful-deep-copy-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "ts-typecheck": "tsc -p ./tsconfig.json --noEmit --skipLibCheck",
    "start": "parcel src/index.html",
    "build": "parcel build src/index.html",
    "format": "prettier --config ./.prettierrc --write src/**/*.tsx",
    "deploy": "parcel build --public-url /contentful-deep-copy-app src/index.html && gh-pages -d dist"
  },
  "dependencies": {
    "@contentful/forma-36-fcss": "^0.0.35",
    "@contentful/forma-36-react-components": "^3.37.1",
    "@contentful/forma-36-tokens": "^0.5.2",
    "contentful": "^7.10.0",
    "contentful-ui-extensions-sdk": "^3.14.2",
    "prop-types": "^15.7.2",
    "rambda": "^5.12.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "axios": "^0.19.2"
  },
  "devDependencies": {
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.7",
    "gh-pages": "^3.1.0",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.0.5",
    "typescript": "^3.8.3"
  },
  "browserslist": [
    "last 5 Chrome version",
    "> 1%",
    "not ie <= 11"
  ]
}
